{
  "name": "@eotlab/dataplatform-webapp",
  "version": "0.1.0",
  "scripts": {
    "build": "ng build",
    "lint": "npm run lint:eslint && npm run lint:format",
    "lint:check": "eslint \"src/**/*.{ts,html}\" --max-warnings=5 && prettier --check \"src/**/*.{ts,html}\"",
    "lint:eslint": "eslint --fix \"src/**/*.{ts,html}\"",
    "lint:format": "prettier --write \"src/**/*.{ts,html}\"",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --poll 2000",
    "prestart:local": "node src/environments/set-local-env.mjs",
    "start:local": "ng serve --host localhost --port 4200 --poll 2000 --configuration dev-local --open ",
    "test": "ng test",
    "watch": "ng build --watch --configuration development",
    "cypress:open": "cypress open --env configFile=dev",
    "cypress:run:e2e": "cypress run --browser chrome --env configFile=ci --e2e",
    "cypress:run:component": "cypress run --browser chrome --env configFile=ci --component"
  },
  "dependencies": {
    "@angular-eslint/schematics": "18.1.0",
    "@angular/animations": "18.0.6",
    "@angular/build": "^18.2.6",
    "@angular/cdk": "18.0.6",
    "@angular/common": "18.0.6",
    "@angular/compiler": "18.0.6",
    "@angular/core": "18.0.6",
    "@angular/forms": "18.0.6",
    "@angular/material": "18.0.6",
    "@angular/platform-browser": "18.0.6",
    "@angular/platform-browser-dynamic": "18.0.6",
    "@angular/router": "18.0.6",
    "@apollo/client": "3.10.8",
    "@ngx-translate/core": "15.0.0",
    "@ngx-translate/http-loader": "8.0.0",
    "@types/lodash": "4.17.6",
    "apexcharts": "^3.54.0",
    "apollo-angular": "7.0.2",
    "graphql": "16.9.0",
    "graphql-tag": "2.12.6",
    "jwt-decode": "4.0.0",
    "lodash": "4.17.21",
    "ng-apexcharts": "1.12.0",
    "ngx-logger": "5.0.12",
    "ngx-socket-io": "4.7.0",
    "rxjs": "7.8.1",
    "zone.js": "0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.0.7",
    "@angular-eslint/builder": "18.1.0",
    "@angular-eslint/eslint-plugin": "18.1.0",
    "@angular-eslint/eslint-plugin-template": "18.1.0",
    "@angular-eslint/template-parser": "18.1.0",
    "@angular/cli": "18.0.7",
    "@angular/compiler-cli": "18.0.6",
    "@cypress/schematic": "2.5.2",
    "@testing-library/cypress": "10.0.2",
    "@types/jasmine": "5.1.4",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "cypress": "13.13.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.3.2",
    "typescript": "5.4.5"
  }
}
